EC2 Creation
terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 5.0"
    }
  }
}
provider "aws" {
  region     = "ap-south-1"
  access_key = "AK0000IJ2DKUYPQ"
  secret_key = "VJx1MJ00000XYiI3qBzEEsDuDeAQLy0r40"
}

resource "aws_instance" "my_ec2_instance" {
    ami = "ami-0767046d1677be5a0"
 instance_type = "t2.micro" 
    tags = {
        Name = "Terraform_EC2SK"
    }
}

